#include "myDS.h"
#include "insert.h"
void my_insert(int num_store){
    Store *p,*pre,*new_node;
    for(p=HEAD;p!=NULL;p=p->next)
        pre=p;
    p=pre;//這樣才是正確的最後位置
    for(int i=0;i<num_store;i++){
        new_node=(Store *)malloc(sizeof(Store));
        scanf("%s %s",&(new_node->name),&(new_node->phonenumber));
        scanf("%lf %lf",&(new_node->longitude),&(new_node->latitude));
        scanf("%f %d %d %lf %d",&(new_node->rating),&(new_node->time[0]),&(new_node->time[1]),&(new_node->distance),&(new_node->price));    
        new_node->next=p->next;
        p->next=new_node;
        p=p->next;
    }    
}
void print(){
    Store *p;
    for(p=HEAD;p!=NULL;p=p->next){
        printf("%d %s\n",p->id,p->name);
    }
}

