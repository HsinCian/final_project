#include "myDS.h"
#include "insert.h"
// HEAD is point to FOODPAPA (id: 0)
void my_insert(int num_store,Store* HEAD, char *Name,char *Phonenumber,double Longitude,double Latitude,float Rating,int time1,int time2,double Distance,int Price){
    Store *p,*new_node;
    for(p=HEAD;p!=NULL;p=p->next);
    for(int i=0;i<num_store;i++){
        new_node=(Store *)malloc(sizeof(Store));
        strcpy(new_node->name,Name);
        strcpy(new_node->phonenumber,Phonenumber);
        new_node->longitude=Longitude;
        new_node->latitude=Latitude;
        new_node->rating=Rating;
        new_node->time[0]=time1;
        new_node->time[1]=time2;
        new_node->distance=Distance;
        new_node->price=Price;        
        new_node->next=p->next;
        p->next=new_node;
        p=p->next;
    }
    
}
